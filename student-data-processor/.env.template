# Database Configuration for Student Data Processor
# Copy this file to .env and update with your actual values

# PostgreSQL Database Connection
DB_HOST=localhost
DB_PORT=5432
DB_NAME=student_db
DB_USERNAME=postgres
DB_PASSWORD=your_actual_password_here

# Server Configuration
SERVER_PORT=8080

# File Paths (adjust for your system)
APP_CSV_OUTPUT_PATH=C:/var/log/applications/API/dataprocessing/
APP_EXCEL_OUTPUT_PATH=C:/var/log/applications/API/dataprocessing/
